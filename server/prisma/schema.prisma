generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User { 
    id                  String             @id @default(cuid())
    email               String             @unique()
    login               String             @unique()
    password            String
    dateBirth           String
}

